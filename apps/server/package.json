{
  "name": "@brain-platform/server",
  "version": "2.7.7",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "echo \"export const version = \\\"$(node -e \"console.log(require('./package.json').version)\")\\\"\" > src/build-info.ts",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "npm run start:dev",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config jest-all.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --config jest-all.json --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config jest-all.json --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:cov": "jest --config ./test/jest-e2e.json --coverage",
    "test:unit": "jest",
    "test:unit:coverage": "jest --coverage",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d ./src/index-migrations.ts -p true",
    "migration:create": "typeorm-ts-node-commonjs migration:create",
    "premigration:run": "nest build",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d ./src/index-migrations.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d ./src/index-migrations.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.600.0",
    "@aws-sdk/s3-request-presigner": "^3.744.0",
    "@google/genai": "1.4.0",
    "@modelcontextprotocol/sdk": "^1.12.1",
    "@nestjs/axios": "4.0.0",
    "@nestjs/common": "^11.1.1",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.1.1",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/platform-express": "^11.1.1",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@rekog/mcp-nest": "^1.5.2",
    "axios": "^1.9.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cron": "^3.1.7",
    "date-fns": "^3.6.0",
    "handlebars": "^4.7.8",
    "pg": "8.11.3",
    "reflect-metadata": "^0.1.12",
    "rxjs": "7.8.1",
    "typeorm": "0.3.20",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.28.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.1",
    "@testcontainers/postgresql": "10.9.0",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/lodash": "4.17.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.10",
    "@types/supertest": "^6.0.3",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "5.4.1",
    "globals": "^16.2.0",
    "jest": "29.5.0",
    "source-map-support": "0.5.21",
    "supertest": "7.1.1",
    "testcontainers": "10.9.0",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.33.0"
  }
}
