{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.ts$": "ts-jest"
  },
  "coverageDirectory": "./coverage/e2e",
  "collectCoverageFrom": ["src/**/*.ts"],
  "coveragePathIgnorePatterns": ["src/.*mock\\.ts", "src/.*spec\\.ts"],
  "maxWorkers": 3
}
