{
  "name": "brain-platform",
  "version": "2.7.7",
  "description": "monorepo for brain platform",
  "scripts": {
    "build": "npm run build --ws",
    "preserver:prod": "npm run server:migrate",
    "server:prod": "npm run start:prod --w apps/server",
    "server:migrate": "npm run migration:config:generate --w apps/server && npm run migration:run --w apps/server",
    "dev": "npm run dev --ws",
    "test": "npm run test --ws",
    "server:test:all": "npm run test --w apps/server",
    "server:test:unit": "npm run test:unit --w apps/server",
    "lint": "npm run lint --ws",
    "format": "npm run format --ws",
    "lint:format": "npm run lint && npm run format",
    "prepare": "husky"
  },
  "workspaces": [
    "apps/server"
  ],
  "keywords": [
    "elands-studio",
    "nestjs",
    "react",
    "brAIn"
  ],
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/aws-lambda": "^8.10.150",
    "@typescript-eslint/eslint-plugin": "8.33.0",
    "@typescript-eslint/parser": "8.33.0",
    "eslint": "^9.28.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.33.1"
  },
  "dependencies": {
    "@aws-sdk/client-apigatewayv2": "^3.829.0",
    "@aws-sdk/client-ec2": "^3.828.0",
    "@aws-sdk/client-ecs": "^3.829.0",
    "aws-lambda": "^1.0.7",
    "chrono-node": "^2.8.3",
    "google-auth-library": "^10.1.0",
    "googleapis": "^150.0.1"
  }
}
